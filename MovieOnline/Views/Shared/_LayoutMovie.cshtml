    <script type="text/javascript">
    $("document").ready(function () {

        $("#submitRegister").click(function () {

            var data = {};


            data.fullnamere = $("#fullnamere").val();
            data.usernamere = $("#usernamere").val();
            data.emailre = $("#emailre").val();
            data.passwordre = $("#passwordre").val();
            data.password_twore = $("#password_twore").val();

            if (data.passwordre !== data.password_twore) {
                $("#er").text("Fail Register");
                return false;
            }

            console.log(data);

            $.ajax({
                type: "POST",
                dataType: "json",
                contentType: "application/json; charset=utf-8",
                data: '{data: ' + JSON.stringify(data) + '}',
                url: '@Url.Action("FormRegister","Home")',
                success: function(kq) {
                    if (kq.Message === "SUCCESS") {
                        location.reload();
                    } else {
                        $(".er").text("Fail Register");
                    }

                },
                error:  function(kq) {

                }

            });
        });




    });
    </script>


